{
  "name": "website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "testBuild": "ng build --configuration test",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "generate-schema": "npm run generate:specs:src && npm run generate:specs:mock",
    "generate:specs:src": "openapi-typescript ../design/openapi.yml --output src/schema.ts --prettier-config ../.prettierrc.yml",
    "generate:specs:mock": "openapi-typescript ../design/openapi.yml --output mock/schema.ts --prettier-config ../.prettierrc.yml",
    "mock": "nodemon --cwd ./mock index.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.3",
    "@angular/common": "^12.0.3",
    "@angular/compiler": "^12.0.3",
    "@angular/core": "^12.0.3",
    "@angular/forms": "^12.0.3",
    "@angular/localize": "^12.0.3",
    "@angular/platform-browser": "^12.0.3",
    "@angular/platform-browser-dynamic": "^12.0.3",
    "@angular/router": "^12.0.3",
    "@aws-amplify/analytics": "^4.0.3",
    "@aws-amplify/auth": "^4.0.3",
    "@aws-amplify/core": "^4.0.3",
    "@aws-sdk/client-cognito-identity": "^3.17.0",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@ng-bootstrap/ng-bootstrap": "^9.1.1",
    "@ngrx/effects": "^12.0.0",
    "@ngrx/store": "^12.0.0",
    "@ngrx/store-devtools": "^12.0.0",
    "bootstrap": "^4.5.0",
    "date-fns": "^2.22.1",
    "maplibre-gl": "^1.14.0",
    "ng-recaptcha": "^9.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.3",
    "@angular/cli": "^12.0.3",
    "@angular/compiler-cli": "^12.0.3",
    "@aws-sdk/types": "^3.15.0",
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.0.0",
    "@types/faker": "^5.5.6",
    "@types/jasmine": "~3.6.0",
    "@types/koa": "^2.13.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa__cors": "^3.0.2",
    "@types/koa__router": "^8.0.4",
    "@types/maplibre-gl": "^1.13.1",
    "@types/node": "^12.20.14",
    "faker": "^5.5.3",
    "jasmine-core": "~3.7.0",
    "karma": "^6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "nodemon": "^2.0.7",
    "openapi-typescript": "^3.4.1",
    "prettier": "^2.3.0",
    "ts-node": "^10.0.0",
    "typescript": "~4.2.3"
  }
